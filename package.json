{
  "name": "brickbox",
  "version": "1.0.0",
  "description": "BrickBox AI System",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_PORT=3000 vite",
    "api": "node --env-file=.env server/training-api.js",
    "worker": "node --env-file=.env server/worker.js",
    "postprocess": "node --env-file=.env scripts/postprocess_worker.js",
    "synthetic": "node --env-file=.env server/synthetic-api.js",
    "dev:full": "concurrently -n WEB,API,WORKER,POST,SYN -c auto \"npm:dev\" \"npm:api\" \"npm:worker\" \"npm:postprocess\" \"npm:synthetic\"",
    "build": "vite build",
    "preview": "cross-env PREVIEW_PORT=4173 vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.12.2",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "localforage": "^1.10.0",
    "onnxruntime-node": "^1.18.0",
    "onnxruntime-web": "^1.18.0",
    "p-limit": "^4.0.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vue": "^3.3.0",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "vite": "^4.4.0"
  }
}

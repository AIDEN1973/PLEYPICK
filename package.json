{
  "name": "brickbox",
  "version": "1.0.0",
  "description": "Vue.js + Supabase application",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"vite,embed,post\" -c \"blue,green,magenta\" \"vite\" \"npm run worker\" \"npm run worker:postprocess\"",
    "dev:no-worker": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/synthetic-api.js",
    "worker": "python scripts/embedding_worker.py",
    "worker:postprocess": "node scripts/postprocess_worker.js",
    "dev:full": "concurrently -n \"vite,server,embed,post\" -c \"blue,yellow,green,magenta\" \"vite\" \"npm run server\" \"npm run worker\" \"npm run worker:postprocess\"",
    "dev:all": "concurrently -n \"vite,embed,postprocess\" -c \"blue,green,magenta\" \"vite\" \"npm run worker\" \"npm run worker:postprocess\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.74.0",
    "chart.js": "^4.5.1",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "localforage": "^1.10.0",
    "node-fetch": "^3.3.2",
    "onnxruntime-web": "^1.23.0",
    "p-limit": "^4.0.0",
    "sharp": "^0.33.2",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vue-tsc": "^3.1.0"
  }
}

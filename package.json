{
  "name": "brickbox",
  "version": "1.0.0",
  "description": "BrickBox AI System",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_PORT=3000 vite --port 3000 --force",
    "api": "node --env-file=.env server/training-api.js",
    "worker": "node --env-file=.env server/worker.js",
    "worker:auto": "node --env-file=.env scripts/start-worker.js",
    "postprocess": "node --env-file=.env scripts/postprocess_worker.js",
    "synthetic": "node --env-file=.env server/synthetic-api.js",
    "synthetic:auto": "node --env-file=.env scripts/start-synthetic-api.js",
    "dev:api": "node --env-file=.env server/synthetic-api.js",
    "manual-upload": "node --env-file=.env server/manual-upload-api.cjs",
    "monitoring": "node --env-file=.env server/monitoring-api.js",
    "inspection-api": "node --env-file=.env server/inspection-api.js",
    "lego-instructions-api": "node --env-file=.env server/lego-instructions-api.js",
    "clip:service": "pip install -r server/requirements-clip.txt && python server/clip-embedding-service.py",
    "ai-api": "node public/api/ai/inference.js",
    "webp-image-api": "node --env-file=.env server/webp-image-api.js",
    "semantic-vector-api": "node --env-file=.env server/semantic-vector-api.js",
    "blender-api": "node --env-file=.env server/blender-api.js",
    "training-executor": "node --env-file=.env server/training-executor.js",
    "port-setup": "node scripts/port-manager.cjs",
    "cleanup": "node scripts/cleanup-ports.cjs",
    "start": "node scripts/optimized-start.js",
    "start:legacy": "node scripts/simple-start.js",
    "dev:full": "npm run cleanup:force && npm run cleanup:vite && npm run port-setup && npm run start",
    "recover": "node scripts/auto-recovery-manager.js",
    "recover:resume": "node scripts/auto-recovery-manager.js --resume-only",
    "monitor": "node scripts/server-monitor.js",
    "dev:fast": "npm run port-setup && node scripts/fast-start.js",
    "dev:quick": "node scripts/fast-start.js",
    "dev:all": "npm run cleanup:force && npm run port-setup && npm run start",
    "dev:ultra": "node scripts/ultra-start-all.js",
    "dev:auto": "node scripts/simple-auto-start.js",
    "dev:full:debug": "npm run cleanup:force && npm run port-setup && echo 'Starting services...' && npx concurrently -n WEB,API,WORKER,POST,SYN,MANUAL,SEMANTIC,BLENDER,TRAINING -c auto --kill-others-on-fail --restart-tries 3 \"npm:dev\" \"npm:api\" \"npm:worker:auto\" \"npm:postprocess\" \"npm:synthetic:auto\" \"npm:manual-upload\" \"npm:semantic-vector-api\" \"npm:blender-api\" \"npm:training-executor\"",
    "start:auto": "node scripts/auto-start-all.js",
    "cleanup:complete": "node scripts/complete-cleanup.js",
    "cleanup:force": "node scripts/force-cleanup-ports.js",
    "cleanup:vite": "rimraf node_modules/.vite dist",
    "ultra-cleaner": "node scripts/ultra-port-cleaner.js",
    "start:fresh": "npm run cleanup:complete && npm run port-setup && npm run start",
    "start:force": "npm run cleanup:force && npm run port-setup && npm run start",
    "build": "vite build",
    "preview": "cross-env PREVIEW_PORT=4173 vite preview"
  },
  "dependencies": {
    "@supabase/node-fetch": "^2.6.13",
    "@supabase/supabase-js": "^2.76.1",
    "@vue/devtools-api": "^8.0.3",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "localforage": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "onnxruntime-node": "^1.18.0",
    "onnxruntime-web": "^1.18.0",
    "p-limit": "^4.0.0",
    "pinia": "^3.0.3",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "vue": "^3.3.0",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^4.4.0"
  }
}
